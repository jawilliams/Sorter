<%= form_for @car do |f| %>
  <%= f.label :color %>
  <%= f.text_field :color %>
  <br />
  <%= f.label :make %>
  <%= f.text_field :make %>
  <br />
  <%= f.label :model %>
  <%= f.text_field :model %>
  <br />
  <%= f.label :year %>
  <%= f.text_field :year %>
  <br />
  <%= f.submit %>
<% end %>
